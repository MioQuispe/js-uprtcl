{
  "name": "@uprtcl/micro-orchestrator",
  "version": "0.0.1-alpha",
  "description": "Library to help coordinate different frontend modules to build entire applications from small building blocks",
  "keywords": [],
  "homepage": "https://github.com/uprtcl/spec/",
  "main": "dist/uprtcl-micro-orchestrator.umd.js",
  "module": "dist/uprtcl-micro-orchestrator.es5.js",
  "typings": "dist/types/uprtcl-micro-orchestrator.d.ts",
  "files": [
    "dist"
  ],
  "author": "guillemcordoba <guillem.cordoba@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/uprtcl/js-uprtcl/"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf dist docs",
    "build": "npm run clean && rollup -c rollup.config.ts",
    "dev": "rollup -c rollup.config.ts -w",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clear-jest": "jest --clearCache",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "docs:build": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "docs:deploy": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.11.0",
    "@types/ws": "6.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^8.0.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.138",
    "inversify": "^5.0.1",
    "lit-element": "^2.2.1",
    "lodash": "^4.17.15",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.4",
    "redux-dynamic-middlewares": "^1.0.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13"
  }
}
